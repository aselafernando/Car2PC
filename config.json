{
  "name":"Car2PCPlugin",
  "label": "Car2PC Settings",
  "config": {
    "type": "loader",
    "iconImage": "qrc:/Car2PCSerial/icons/cd-100-white.png",
    "source": "qrc:/Car2PCSerial/SerialSettings.qml",
    "settings": [
      {
        "name": "serial_port",
        "type": "string",
        "defaultValue": "/dev/car2pc"
      },
      {
        "name": "text",
        "type": "switch",
        "textOff": "Off",
        "textOn": "On",
        "defaultValue": false
      },
      {
        "name": "FF_ON",
        "type": "string",
        "defaultValue": ""
      },
      {
        "name": "FF_OFF",
        "type": "string",
        "defaultValue": ""
      },
      {
        "name": "RW_ON",
        "type": "string",
        "defaultValue": ""
      },
      {
        "name": "RW_OFF",
        "type": "string",
        "defaultValue": ""
      },
      {
        "name": "SCAN_ON",
        "type": "string",
        "defaultValue": "PhoneBluetooth::VoiceRecognition"
      },
      {
        "name": "SCAN_OFF",
        "type": "string",
        "defaultValue": "PhoneBluetooth::VoiceRecognition"
      },
      {
        "name": "PREV_DISC",
        "type": "string",
        "defaultValue": "PhoneBluetooth::Hangup"
      },
      {
        "name": "NEXT_DISC",
        "type": "string",
        "defaultValue": "PhoneBluetooth::Answer"
      }
    ]
  }
}
